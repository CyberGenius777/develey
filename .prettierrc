{
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "auto",
  "tabWidth": 2,
  "useTabs": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2,
        "singleQuote": false,
        "useTabs": false
      }
    }
  ]
}
